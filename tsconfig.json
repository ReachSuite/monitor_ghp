{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "lib": ["ES2021", "dom", "DOM.Iterable"],
    "baseUrl": ".",
    "esModuleInterop": true,
    "strict": true,
    "allowJs": false,
    "jsx": "react",
    "resolveJsonModule": true,
    "noEmit": true,
    "noImplicitOverride": true,
    "useUnknownInCatchVariables": false
  },
  "compileOnSave": true
}
